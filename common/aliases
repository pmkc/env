alias sl="sl -aelF"
alias chrome=google-chrome
alias mctemp='cd $(mktemp -d)'
alias sp=". $HOME/.profile"
alias tm='tmux detach; tmux -2 a || tmux -2 new'
alias de='disown && exit'
alias cde='google-chrome & disown && exit'
alias oops='history -d $((HISTCMD-2)); clear'
alias mvn='mvn -T1C'
alias emacs-gtk=emacs
alias xcape="pgrep xcape || xcape -e 'Caps_Lock=Escape;Control_L=Escape'"
alias vd=vimdiff
alias unhide="rename 's/^.//'"
alias hide="rename 's/^/./'"
